{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Vite",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"useDefineForClassFields": true,
		"sourceMap": true,
		"resolveJsonModule": true,
		"noEmit": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true
	}
}
