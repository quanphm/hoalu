{
	"$schema": "https://turbo.build/schema.json",
  "ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["build/**", "dist/**", ".output/**"]
		},
		"format": {},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
